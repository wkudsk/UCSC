@;;File: 21-let-if.sb
;;   1: # $Id: 21-let-if.sbir,v 1.1 2017-08-21 17:29:41-07 - - $
;;   2: #
;;   3: 	let i = 1
;;   4: loop:	print i
;;   5: 	let i = i + 1
;;   6: 	if i <= 10 goto loop
(
(    1          )
(    2          )
(    3          (let i 1))
(    4 loop     (print i))
(    5          (let i (+ i 1)))
(    6          (if (<= i 10) loop))
)
